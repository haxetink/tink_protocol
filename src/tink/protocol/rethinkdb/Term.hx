class TermArrayTools {
		
	public static function asString(v:Array<Term>):String {
		return '[' + [for(i in v) i.asString()].join(',') + ']';
	}
	
}

class TermObjectTools {
	
	public static function asString(v:Array<Named<Term>>):String
		return '{' + [for(o in v) '"${o.name}":${o.value.asString()}'].join(',') + '}';
		
}

